#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yjaiswal/language-server/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.8.10/node_modules/jest-cli/bin/node_modules:/home/yjaiswal/language-server/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.8.10/node_modules/jest-cli/node_modules:/home/yjaiswal/language-server/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.8.10/node_modules:/home/yjaiswal/language-server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yjaiswal/language-server/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.8.10/node_modules/jest-cli/bin/node_modules:/home/yjaiswal/language-server/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.8.10/node_modules/jest-cli/node_modules:/home/yjaiswal/language-server/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.8.10/node_modules:/home/yjaiswal/language-server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jest.js" "$@"
else
  exec node  "$basedir/../../bin/jest.js" "$@"
fi
